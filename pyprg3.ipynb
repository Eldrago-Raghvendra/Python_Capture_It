    "def reversing(l):\n",
    "    l5=[]\n",
    "    for i in range(len(l)-1,-1,-1):\n",
    "        l5.append(l[i])\n",
    "    return l5    \n",
    "def sum(l):\n",
    "    cnt=0\n",
    "    n=len(l)\n",
    "    for i in range(1,n):\n",
    "        cnt=cnt+l[i]\n",
    "    return cnt   \n",
    "l = [1, 11, 21, 31, 41] \n",
    "print(reversing(l))\n",
    "print(sum(l))"
   ]
